  "devDependencies": {
    "@types/node": "^16.7.10",
    "@types/websocket": "^1.0.4",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "websocket": "^1.0.34"
  }
}
