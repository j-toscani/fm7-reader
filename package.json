{
  "name": "fm7-reader",
  "version": "1.0.0",
  "description": "A UDP-Streamreader written in NodeJS for Forza Motorsport 7",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "convert": "ts-node ./src/converter/index.ts",
